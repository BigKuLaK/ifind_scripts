{
  "kind": "collectionType",
  "collectionName": "product_attributes",
  "info": {
    "name": "Product Attribute",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": false
    },
    "code": {
      "type": "string",
      "required": false
    },
    "is_common": {
      "type": "boolean",
      "default": true,
      "required": false
    },
    "data_type": {
      "type": "enumeration",
      "enum": [
        "number",
        "date_time"
      ],
      "default": "number"
    },
    "custom_formula": {
      "type": "string"
    },
    "disable_min": {
      "type": "boolean",
      "default": false
    },
    "disable_max": {
      "type": "boolean",
      "default": false
    },
    "min_label": {
      "type": "string",
      "default": "Min"
    },
    "max_label": {
      "type": "string",
      "default": "Max"
    },
    "product_prop": {
      "type": "enumeration",
      "enum": [
        "none",
        "price",
        "release_date"
      ],
      "default": "none"
    }
  }
}
